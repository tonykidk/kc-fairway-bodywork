# Supabase Configuration
# Copy this file to .env and replace with your actual Supabase project credentials

# Your Supabase project URL (found in Settings > API)
# This will be available on both client and server
PUBLIC_SUPABASE_URL=your_supabase_project_url_here

# Your Supabase anon/public key (found in Settings > API)
# This will be available on both client and server
PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Service role key for server-side operations (keep secret!)
# This will only be available on the server
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here 